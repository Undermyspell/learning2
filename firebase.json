{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/api/**",
      "run": {
        "serviceId": "mknode",
        "region": "europe-west1"
      }
    }, {
      "source": "**",
      "run": {
        "serviceId": "mkangular",
        "region": "europe-west1"
      }
    }]
  }
}
